keybind "Ctrl-A" {
  command = "layout.toggleOrientation"
}

keybind "Shift-A" {
  exec = "xterm"
}

keybind "Ctrl-Up" {
  command = "layout.adjustLeft"
}

keybind "Ctrl-Left" {
  command = "layout.adjustLeft"
}

keybind "Ctrl-Down" {
  command = "layout.adjustRight"
}

keybind "Ctrl-Right" {
  command = "layout.adjustRight"
}
